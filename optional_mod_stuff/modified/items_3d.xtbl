<TableDescription>
	<Name>Item</Name>
	<Type>TableDescription</Type>
	<Display_Name>3D Item</Display_Name>
	<Description>A 3D item (AKA: "object_item" in the code) represents an object that is placed in the world somehow. It may have been dropped by a human
(on death, for instance) or placed through scripting or code.</Description>
	<Element>
		<Name>Name</Name>
		<Type>String</Type>
		<Display_Name>3D Item Name</Display_Name>
		<Description>Unique name of the 3D item, used internally by the game.</Description>
		<Unique>true</Unique>
		</Element>
	<Element>
		<Name>DisplayName</Name>
		<Type>Reference</Type>
		<Display_Name>Display Name</Display_Name>
		<Required>false</Required>
		<Reference>
			<File>localized_exports\Item_text.xtbl</File>
			<Type>Items_Identifier.Name</Type>
			<OpenSeparate>False</OpenSeparate>
			</Reference>
		</Element>
	<Element>
		<Name>Item_Flags</Name>
		<Type>Flags</Type>
		<Display_Name>Item Flags</Display_Name>
		<Description>Flags for the item.</Description>
		<Required>false</Required>
		<Flag>spin</Flag>
		<Flag>collide_as_sphere</Flag>
		<Flag>keep</Flag>
		<Flag>use_pickup</Flag>
		<Flag>is_drug</Flag>
		<Flag>collide_as_capsule</Flag>
		<Flag>is_money</Flag>
		<Flag>collide_all</Flag>
		<Flag>no_auto_destroy</Flag>
		<Flag>dont_stick_to_ground</Flag>
		<Flag>is_walker</Flag>
		<Flag>dont_drop_from_prop</Flag>
		</Element>
	<Element>
		<Name>Mesh</Name>
		<Type>Filename</Type>
		<Display_Name>Static Mesh</Display_Name>
		<Description>Filename of static mesh to use for the 3D item.</Description>
		<Extension>smeshx</Extension>
		<StartingPath>data</StartingPath>
		<ShowPreload>True</ShowPreload>
		</Element>
	<Element>
		<Name>LargeProp</Name>
		<Type>Selection</Type>
		<Display_Name>Large Prop</Display_Name>
		<Default>False</Default>
		<Choice>True</Choice>
		<Choice>False</Choice>
		</Element>
	<Element>
		<Name>NoAttach</Name>
		<Type>Selection</Type>
		<Display_Name>No Attach To Anims</Display_Name>
		<Description>This item may be attached to an animation, but the attachment is only for streaming purposes.  Not for attachment purposes.  Needed for LIFE system.</Description>
		<Required>false</Required>
		<Default>False</Default>
		<Choice>True</Choice>
		<Choice>False</Choice>
		</Element>
	<Element>
		<Name>Touch_Script</Name>
		<Type>Selection</Type>
		<Display_Name>Touch Script</Display_Name>
		<Description>Name of hard-coded script to initiate when item is "touched".</Description>
		<Required>false</Required>
		<Default>give_item</Default>
		<Choice>give_item</Choice>
		<Choice>give_rand_money</Choice>
		<Choice>give_money</Choice>
		<Choice>give_drugs</Choice>
		<Choice>give_collectible_item</Choice>
		<Choice>give_mission_item</Choice>
		</Element>
	<Element>
		<Name>FoleyTouch</Name>
		<Type>Reference</Type>
		<Display_Name>Foley Touch</Display_Name>
		<Description>Name of foley touch entry to use when item is touched.</Description>
		<Required>false</Required>
		<Reference>
			<File>foley_touch.xtbl</File>
			<Type>FoleyTouch.Name</Type>
			<OpenSeparate>False</OpenSeparate>
			</Reference>
		</Element>
	<Element>
		<Name>FoleyCollision</Name>
		<Type>Reference</Type>
		<Display_Name>Foley Collision</Display_Name>
		<Description>What collision foley sound set do we use?</Description>
		<Required>false</Required>
		<Reference>
			<File>foley_collision.xtbl</File>
			<Type>FoleyCollision.Name</Type>
			<OpenSeparate>False</OpenSeparate>
			</Reference>
		</Element>
	<Element>
		<Name>Mass</Name>
		<Type>Float</Type>
		<Description>Object mass - required for projectiles.</Description>
		<Required>false</Required>
		<Default>0.0</Default>
		</Element>
	<Element>
		<Name>Respawn_Delay</Name>
		<Type>Int</Type>
		<Display_Name>Respawn Delay</Display_Name>
		<Description>Time (in milliseconds) between respawns.</Description>
		<Required>false</Required>
		<Default>0</Default>
		</Element>
	<Element>
		<Name>Glow_Type</Name>
		<Type>Selection</Type>
		<Display_Name>Glow Type</Display_Name>
		<Default>None</Default>
		<Choice>None</Choice>
		<Choice>Weapons</Choice>
		<Choice>Cash</Choice>
		<Choice>Drugs</Choice>
		</Element>
	<Element>
		<Name>Render_Scale</Name>
		<Type>Float</Type>
		<Display_Name>Render Scale</Display_Name>
		<Description>A scale factor at which to render this item when it is on the ground.  1.0 is normal size.</Description>
		<Required>false</Required>
		<Default>1.0</Default>
		<MinValue>0.1</MinValue>
		</Element>
	<Element>
		<Name>Scale_Ambient</Name>
		<Type>Selection</Type>
		<Display_Name>Scale Ambient</Display_Name>
		<Default>false</Default>
		<Choice>true</Choice>
		<Choice>false</Choice>
		</Element>
	<Element>
		<Name>Wieldable</Name>
		<Type>Element</Type>
		<Description>Whether this item can be collected and used as a melee weapon or thrown</Description>
		<Required>false</Required>
		<Element>
			<Name>Shape</Name>
			<Type>Selection</Type>
			<Default>2-hand</Default>
			<Choice>2-hand</Choice>
			<Choice>1-hand</Choice>
			<Choice>2-hand low</Choice>
			<Choice>2-hand high</Choice>
			</Element>
		<Element>
			<Name>Melee_Damage</Name>
			<Type>Float</Type>
			<Default>50</Default>
			</Element>
		</Element>
	<Element>
		<Name>Collectible</Name>
		<Type>Element</Type>
		<Description>Gotta collect them all!</Description>
		<Required>false</Required>
		<Element>
			<Name>Threshold_reward_grid</Name>
			<Type>Grid</Type>
			<Description>Unlockables rewarded to the player after the specified number of items have been collected.</Description>
			<Required>false</Required>
			<Element>
				<Name>Threshold_reward</Name>
				<Type>Element</Type>
				<Required>false</Required>
				<Element>
					<Name>Threshold</Name>
					<Type>Int</Type>
					<Description>The number of items which must be collected for this reward.</Description>
					<Default>0</Default>
					</Element>
				<Element>
					<Name>Unlockable</Name>
					<Type>Reference</Type>
					<Description>Unlocked when the corresponding collection threshold is reached.</Description>
					<Reference>
						<File>unlockables.xtbl</File>
						<Type>Unlockable.Name</Type>
						<OpenSeparate>False</OpenSeparate>
						</Reference>
					</Element>
				</Element>
			<Num_Display_Rows>8</Num_Display_Rows>
			</Element>
		<Element>
			<Name>Complete_collection_unlockable</Name>
			<Type>Reference</Type>
			<Description>Reward unlocked when the entire set is collected.</Description>
			<Required>false</Required>
			<Reference>
				<File>unlockables.xtbl</File>
				<Type>Unlockable.Name</Type>
				<OpenSeparate>False</OpenSeparate>
				</Reference>
			</Element>
		</Element>
	<Element>
		<Name>LODs</Name>
		<Type>Grid</Type>
		<Display_Name>Levels of Detail</Display_Name>
		<Required>false</Required>
		<Element>
			<Name>LOD_Distance</Name>
			<Type>Float</Type>
			<Display_Name>LOD Distance</Display_Name>
			<Default>10</Default>
			</Element>
		<Max_Children>4</Max_Children>
		<Num_Display_Rows>4</Num_Display_Rows>
		</Element>
	<Element>
		<Name>Color_Variants</Name>
		<Type>Grid</Type>
		<Display_Name>Color Variants</Display_Name>
		<Element>
			<Name>Color_Variant</Name>
			<Type>Element</Type>
			<Display_Name>Color Variant</Display_Name>
			<Element>
				<Name>Material_Name</Name>
				<Type>String</Type>
				<Display_Name>Material Name</Display_Name>
				</Element>
			<Element>
				<Name>Variable_Name</Name>
				<Type>String</Type>
				<Display_Name>Variable Name</Display_Name>
				</Element>
			<Element>
				<Name>Color_Options</Name>
				<Type>Grid</Type>
				<Display_Name>Color Options</Display_Name>
				<Element>
					<Name>Color_Option</Name>
					<Type>Reference</Type>
					<Display_Name>Option</Display_Name>
					<Reference>
						<File>items_color_pool.xtbl</File>
						<Type>Item_Color.Name</Type>
						<OpenSeparate>False</OpenSeparate>
						</Reference>
					</Element>
				<Num_Display_Rows>8</Num_Display_Rows>
				</Element>
			</Element>
		<Num_Display_Rows>8</Num_Display_Rows>
		</Element>
	</TableDescription>

