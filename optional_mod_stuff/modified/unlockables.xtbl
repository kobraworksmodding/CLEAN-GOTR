			<Required>false</Required>
			<Element>
				<Name>Name</Name>
				<Type>Reference</Type>
				<Description>Crib weapon to be made available in the crib.</Description>
				<Reference>
					<File>weapons.xtbl</File>
					<Type>Weapon.Name</Type>
					<OpenSeparate>False</OpenSeparate>
					</Reference>
				</Element>
			</Element>
