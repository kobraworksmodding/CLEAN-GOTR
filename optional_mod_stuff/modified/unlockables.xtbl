<TableDescription>
	<Name>Unlockable</Name>
	<Type>TableDescription</Type>
	<Element>
		<Name>Name</Name>
		<Type>String</Type>
		<Description>The unique name of the unlockable.</Description>
		</Element>
	<Element>
		<Name>Type</Name>
		<Type>ComboElement</Type>
		<Description>The type of thing to unlock.</Description>
		<Element>
			<Name>Clothes</Name>
			<Type>Element</Type>
			<Required>false</Required>
			<Element>
				<Name>Items</Name>
				<Type>Grid</Type>
				<Element>
					<Name>Item</Name>
					<Type>Element</Type>
					<Element>
						<Name>ItemName</Name>
						<Type>Reference</Type>
						<Display_Name>Name</Display_Name>
						<Description>Clothing item to be unlocked</Description>
						<Reference>
							<File>customization_items.xtbl</File>
							<Type>Customization_Item.Name</Type>
							<OpenSeparate>False</OpenSeparate>
							</Reference>
						</Element>
					<Element>
						<Name>ItemVariant</Name>
						<Type>String</Type>
						<Display_Name>Variant</Display_Name>
						<Description>Variant of the item to unlock</Description>
						</Element>
					<Element>
						<Name>Color1</Name>
						<Type>Reference</Type>
						<Display_Name>Color 1</Display_Name>
						<Required>false</Required>
						<Reference>
							<File>character_color_pool.xtbl</File>
							<Type>Color_Entry.Name</Type>
							<OpenSeparate>False</OpenSeparate>
							</Reference>
						</Element>
					<Element>
						<Name>Color2</Name>
						<Type>Reference</Type>
						<Display_Name>Color 2</Display_Name>
						<Required>false</Required>
						<Reference>
							<File>character_color_pool.xtbl</File>
							<Type>Color_Entry.Name</Type>
							<OpenSeparate>False</OpenSeparate>
							</Reference>
						</Element>
					<Element>
						<Name>Color3</Name>
						<Type>Reference</Type>
						<Display_Name>Color 3</Display_Name>
						<Required>false</Required>
						<Reference>
							<File>character_color_pool.xtbl</File>
							<Type>Color_Entry.Name</Type>
							<OpenSeparate>False</OpenSeparate>
							</Reference>
						</Element>
					</Element>
				<Num_Display_Rows>8</Num_Display_Rows>
				</Element>
			</Element>
		<Element>
			<Name>Crib</Name>
			<Type>Element</Type>
			<Description>Unlock a crib.</Description>
			<Required>false</Required>
			<Element>
				<Name>Name</Name>
				<Type>Reference</Type>
