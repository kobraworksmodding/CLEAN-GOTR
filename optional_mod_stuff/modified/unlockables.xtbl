			<Name>Custom</Name>
			<Type>Element</Type>
			<Description>For custom unlockables.</Description>
			<Required>false</Required>
			</Element>
		<Element>
			<Name>Damage</Name>
			<Type>Element</Type>
			<Description>Unlock a resistance to damage.</Description>
			<Required>false</Required>
			<Element>
				<Name>Source</Name>
				<Type>Selection</Type>
