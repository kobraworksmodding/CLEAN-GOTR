<TableDescription>
	<Name>Action_node_npc</Name>
	<Type>TableDescription</Type>
	<Display_Name>Action node npc</Display_Name>
	<Description>If you want more than 60, a programmer must bump MAX_ACTION_NODE_NPC_TYPES</Description>
	<MaxCount>260</MaxCount>
	<Element>
		<Name>Name</Name>
		<Type>String</Type>
		<Unique>true</Unique>
		<MaxLength>20</MaxLength>
		</Element>
	<Element>
		<Name>Animations</Name>
		<Type>Grid</Type>
		<Element>
			<Name>State</Name>
			<Type>Element</Type>
			<Element>
				<Name>Animation_State</Name>
				<Type>Reference</Type>
				<Display_Name>Animation State</Display_Name>
				<MaxCount>4</MaxCount>
				<Reference>
					<File>anim_states.xtbl</File>
					<Type>State.Name</Type>
					<OpenSeparate>False</OpenSeparate>
					</Reference>
				</Element>
			<Element>
				<Name>Actions</Name>
				<Type>Grid</Type>
				<Element>
					<Name>Animation_action</Name>
					<Type>Reference</Type>
					<Display_Name>Animation Action</Display_Name>
					<MaxCount>4</MaxCount>
					<Reference>
						<File>anim_actions.xtbl</File>
						<Type>Action.Name</Type>
						<OpenSeparate>False</OpenSeparate>
						</Reference>
					</Element>
				<Max_Children>4</Max_Children>
				<Min_Children>0</Min_Children>
				<Num_Display_Rows>8</Num_Display_Rows>
				</Element>
			</Element>
		<Max_Children>4</Max_Children>
		<Min_Children>0</Min_Children>
		<Num_Display_Rows>8</Num_Display_Rows>
		</Element>
	<Element>
		<Name>CowerAnims</Name>
		<Type>Grid</Type>
		<Required>false</Required>
		<Element>
			<Name>CowerAnim</Name>
			<Type>Reference</Type>
			<Reference>
				<File>anim_states.xtbl</File>
				<Type>State.Name</Type>
				<OpenSeparate>False</OpenSeparate>
				</Reference>
			</Element>
		<Max_Children>3</Max_Children>
		<Min_Children>0</Min_Children>
		<Num_Display_Rows>8</Num_Display_Rows>
		</Element>
	<Element>
		<Name>Enter_anim</Name>
		<Type>Reference</Type>
		<Display_Name>Enter animation</Display_Name>
		<Description>Animation that will be played when a pedestrian enters this special spawn.</Description>
		<Required>false</Required>
		<Reference>
			<File>anim_actions.xtbl</File>
			<Type>Action.Name</Type>
			<OpenSeparate>False</OpenSeparate>
			</Reference>
		</Element>
	<Element>
		<Name>Exit_anim</Name>
		<Type>Reference</Type>
		<Display_Name>Exit animation</Display_Name>
		<Description>Animation that will be played when a pedestrian enters this special spawn.</Description>
		<Required>false</Required>
		<Reference>
			<File>anim_actions.xtbl</File>
			<Type>Action.Name</Type>
			<OpenSeparate>False</OpenSeparate>
			</Reference>
		</Element>
	<Element>
		<Name>Startle_anim</Name>
		<Type>Reference</Type>
		<Display_Name>Startle animation</Display_Name>
		<Description>Will override normal animation action when the character is startled by gunfire, etc.</Description>
		<Required>false</Required>
		<Reference>
			<File>anim_actions.xtbl</File>
			<Type>Action.Name</Type>
			<OpenSeparate>False</OpenSeparate>
			</Reference>
		</Element>
	<Element>
		<Name>Min_anim_delay</Name>
		<Type>Float</Type>
		<Display_Name>Min animation delay</Display_Name>
		<Description>Minimum time that will elapse before a selected animation is played again.</Description>
		<Default>5.0</Default>
		<MinValue>0</MinValue>
		</Element>
	<Element>
		<Name>Max_anim_delay</Name>
		<Type>Float</Type>
		<Display_Name>Max animation delay</Display_Name>
		<Description>Maximum time that will elapse before a selected animation is played again.</Description>
		<Default>10.0</Default>
		<MinValue>0</MinValue>
		</Element>
	<Element>
		<Name>Persona_situation</Name>
		<Type>Reference</Type>
		<Display_Name>Persona situation</Display_Name>
		<Description>Determines the set of lines this special spawn character will play when they play their action animation.</Description>
		<Required>false</Required>
		<Reference>
			<File>persona_situations.xtbl</File>
			<Type>Situation_List.Situations.Situation.Type</Type>
			<OpenSeparate>False</OpenSeparate>
			</Reference>
		</Element>
	<Element>
		<Name>Model_List</Name>
		<Type>Grid</Type>
		<Display_Name>Model List</Display_Name>
		<Required>false</Required>
		<MaxCount>4</MaxCount>
		<Element>
			<Name>Model</Name>
			<Type>Reference</Type>
			<Reference>
				<File>character_presets.xtbl</File>
				<Type>Preset.Name</Type>
				<OpenSeparate>False</OpenSeparate>
				</Reference>
			</Element>
		<Num_Display_Rows>4</Num_Display_Rows>
		</Element>
	<Element>
		<Name>Num_Resource_Requests</Name>
		<Type>Int</Type>
		<Display_Name>Num Resource Requests</Display_Name>
		<Description>If this is optionally selected, then this determines how many resource requests can be made from the Model List.</Description>
		<Required>false</Required>
		<Default>1</Default>
		<MinValue>1</MinValue>
		<MaxValue>4</MaxValue>
		</Element>
	<Element>
		<Name>Sex</Name>
		<Type>Selection</Type>
		<Required>false</Required>
		<Default>male</Default>
		<Choice>male</Choice>
		<Choice>female</Choice>
		</Element>
	<Element>
		<Name>Team</Name>
		<Type>Selection</Type>
		<Required>false</Required>
		<Choice>Police</Choice>
		<Choice>Civilian</Choice>
		<Choice>Gang</Choice>
		<Choice>Saints</Choice>
		</Element>
	<Element>
		<Name>Required_NPC</Name>
		<Type>Selection</Type>
		<Display_Name>Required NPC</Display_Name>
		<Description>For multi-person spawns.  If a required NPC doesn't spawn, nobody spawns.  If a required NPC despawns, everybody despawns.</Description>
		<Required>false</Required>
		<Default>False</Default>
		<Choice>True</Choice>
		<Choice>False</Choice>
		</Element>
	<Element>
		<Name>Spawn_Priority</Name>
		<Type>Selection</Type>
		<Display_Name>Spawn Priority</Display_Name>
		<Description>Sometimes you need a lot of peds using action nodes (e.g. a night club).  Do you steal from the sidewalks to create a crowd of action node users?</Description>
		<Required>false</Required>
		<Default>Sidewalk (normal)</Default>
		<Choice>Sidewalk (normal)</Choice>
		<Choice>Action Node (high)</Choice>
		</Element>
	<Element>
		<Name>Synced_Animation</Name>
		<Type>Selection</Type>
		<Display_Name>Synced Animation</Display_Name>
		<Required>false</Required>
		<Default>False</Default>
		<Choice>True</Choice>
		<Choice>False</Choice>
		</Element>
	<Element>
		<Name>Single_Use</Name>
		<Type>Selection</Type>
		<Display_Name>Single Use</Display_Name>
		<Required>false</Required>
		<Default>False</Default>
		<Choice>True</Choice>
		<Choice>False</Choice>
		</Element>
	<Element>
		<Name>Flee_On_Exit</Name>
		<Type>Selection</Type>
		<Display_Name>Flee On Exit</Display_Name>
		<Description>Used with Single Use.  Pedestrian will flee after performing their action.Ignored if single  use is not checked.</Description>
		<Required>false</Required>
		<Default>False</Default>
		<Choice>True</Choice>
		<Choice>False</Choice>
		</Element>
	<Element>
		<Name>Follow_Nodes</Name>
		<Type>Grid</Type>
		<Display_Name>Follow Nodes</Display_Name>
		<Description>Types of NPC nodes this NPC should use directly after leaving this node</Description>
		<Required>false</Required>
		<Element>
			<Name>Node</Name>
			<Type>Reference</Type>
			<Reference>
				<File>action_node_npcs.xtbl</File>
				<Type>Action_node_npc.Name</Type>
				<OpenSeparate>False</OpenSeparate>
				</Reference>
			</Element>
		<Num_Display_Rows>8</Num_Display_Rows>
		</Element>
	<Element>
		<Name>Random_destination_percent</Name>
		<Type>Float</Type>
		<Display_Name>Random destination percent</Display_Name>
		<Description>Percentage of time the NPC leaving this node will not go to a follow node and go to a random node instead. Ignored if no follow nodes</Description>
		<Required>false</Required>
		<Default>0.0</Default>
		</Element>
	<Element>
		<Name>Get_Bag</Name>
		<Type>Selection</Type>
		<Display_Name>Get Bag</Display_Name>
		<Required>false</Required>
		<Default>False</Default>
		<Choice>True</Choice>
		<Choice>False</Choice>
		</Element>
	<Element>
		<Name>Can_Use_With_Bag</Name>
		<Type>Selection</Type>
		<Display_Name>Can Use With Bag</Display_Name>
		<Required>false</Required>
		<Default>False</Default>
		<Choice>True</Choice>
		<Choice>False</Choice>
		</Element>
	<Element>
		<Name>Reverse_Enter</Name>
		<Type>Selection</Type>
		<Display_Name>Reverse Enter</Display_Name>
		<Required>false</Required>
		<Default>False</Default>
		<Choice>True</Choice>
		<Choice>False</Choice>
		</Element>
	<Element>
		<Name>ExitOnItemDislodge</Name>
		<Type>Selection</Type>
		<Display_Name>Exit On Item Dislodge</Display_Name>
		<Description>If an item, such as the inmate scrub bucket get knocked, should the person leave the node?</Description>
		<Required>false</Required>
		<Default>False</Default>
		<Choice>True</Choice>
		<Choice>False</Choice>
		</Element>
	<Element>
		<Name>Never_Exit</Name>
		<Type>Selection</Type>
		<Display_Name>Never Exit Voluntarily</Display_Name>
		<Description>Npcs should never choose to leave this node. They can be caused to flee it however</Description>
		<Required>false</Required>
		<Default>False</Default>
		<Choice>True</Choice>
		<Choice>False</Choice>
		</Element>
	<Element>
		<Name>Can_Not_Exit</Name>
		<Type>Selection</Type>
